{
  "env": {
    "commonjs": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "google", "plugin:prettier/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "off"
  }
}
